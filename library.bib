	author = {Wiedmann, Thomas and Lenzen, Manfred and Turner, Karen and Barrett, John},
	month = feb,
	year = {2007},
	keywords = {Ecological Footprint, Embodied environmental impacts, International input{\textendash}output analysis, International trade, Multi-region input{\textendash}output models},
	pages = {15--26},
	file = {ScienceDirect Full Text PDF:/home/rob/.mozilla/firefox/mwad0hks.default/zotero/storage/98X2A4ZJ/Wiedmann et al. - 2007 - Examining the global environmental impact of regio.pdf:application/pdf;ScienceDirect Snapshot:/home/rob/.mozilla/firefox/mwad0hks.default/zotero/storage/KSKDA48Z/S0921800906005945.html:text/html}
