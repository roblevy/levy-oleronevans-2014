@article{blochl_vertex_2011,
	title = {Vertex centralities in input-output networks reveal the structure of modern economies},
	volume = {83},
	url = {http://link.aps.org/doi/10.1103/PhysRevE.83.046127},
	doi = {10.1103/PhysRevE.83.046127},
	abstract = {Input-output tables describe the flows of goods and services between the sectors of an economy. These tables can be interpreted as weighted directed networks. At the usual level of aggregation, they contain nodes with strong self-loops and are almost completely connected. We derive two measures of node centrality that are well suited for such networks. Both are based on random walks and have interpretations as the propagation of supply shocks through the economy. Random walk centrality reveals the vertices most immediately affected by a shock. Counting betweenness identifies the nodes where a shock lingers longest. The two measures differ in how they treat self-loops. We apply both to data from a wide set of countries and uncover salient characteristics of the structures of these national economies. We further validate our indices by clustering according to sectors{\textquoteright} centralities. This analysis reveals geographical proximity and similar developmental status.},
	number = {4},
	urldate = {2014-01-08},
	journal = {Physical Review E},
	author = {Bl{\"o}chl, Florian and Theis, Fabian J. and Vega-Redondo, Fernando and Fisher, Eric O{\textquoteright}N.},
	month = apr,
	year = {2011},
	pages = {046127},
	file = {APS Snapshot:/home/rob/.mozilla/firefox/mwad0hks.default/zotero/storage/GQRHMI8P/e046127.html:text/html}
}

@article{economides_does_2008,
	title = {Does Foreign Aid Distort Incentives and Hurt Growth? Theory and Evidence from 75 Aid-Recipient Countries},
	volume = {134},
	copyright = {Copyright {\textcopyright} 2008 Springer},
	issn = {0048-5829},
	shorttitle = {Does Foreign Aid Distort Incentives and Hurt Growth?},
	url = {http://www.jstor.org/stable/27698237},
	doi = {10.2307/27698237},
	abstract = {Foreign aid transfers can distort individual incentives, and hence hurt growth, by encouraging rent seeking as opposed to productive activities. We construct a model of a growing small open economy that distinguishes two effects from foreign transfers: (i) a direct positive effect, as higher transfers allow the financing of infrasturcture; (ii) an indirect negative effect, as higher transfers induce rent-seeking competition by self-interested individuals. In this framework, the growth impact of aid is examined jointly with the determination of rent-seeking behavior. We test the main predictions of the model for a crosssection of 75 aid-recipient countries. There is evidence that aid has a direct positive effect on growth, which is however significantly mitigated by the adverse indirect effects of associated rent-seeking activities. This is especially the case in recipient countries with relatively large public sectors.},
	number = {3/4},
	urldate = {2013-06-05},
	journal = {Public Choice},
	author = {Economides, George and Kalyvitis, Sarantis and Philippopoulos, Apostolis},
	month = mar,
	year = {2008},
	note = {{ArticleType}: research-article / Full publication date: Mar., 2008 / Copyright {\textcopyright} 2008 Springer},
	pages = {463--488},
